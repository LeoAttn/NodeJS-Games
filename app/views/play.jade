extends layout

block content
    h1(onclick="testlog()")= title

    table(style="border-collapse: collapse;")
        - for (var y = 0; y < 10; y++)
            tr
                - for (var x = 0; x < 10; x++)
                    td(class=x + "-" + y style="border: 1px solid black; font-size: 15px")= x + " - " + y

    img(src="/images/tux.png" width="250px")

    script(src="/javascripts/test.js")